<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="Symfony\Bundle\AsseticBundle\Command\DumpCommand">
            <tag name="console.command" command="assetic:dump" />
        </service>
        
        <service id="Symfony\Bundle\AsseticBundle\Command\WatchCommand">
            <tag name="console.command" command="assetic:watch" />
        </service>
        
    </services>
</container>
